<%= render 'navbar' %>
<table class="table table-striped" id="testresult-table">
  <thead>
    <tr>
      <th>#</th>
      <th>Test Case Name</th>
      <th>Result</th>
      <th>Last Run</th>
      <th>Environment</th>
      <th>Error Message</th>
    </tr>
  </thead>
  <tbody>
    <% i = 1 %>
    <% @testresults.each do |testresult| %>
      <tr>
        <th scope="row"><%= i %></th>
        <td><%= testresult.test_case_name %></td>
        <td><%= testresult.test_result %></td>
        <td><%= testresult.created_at %></td>
        <td><%= testresult.environment %></td>
        <td><%= testresult.error_msg %></td>
      </tr>
      <% i = i + 1 %>
    <% end %>
  </tbody>
</table>